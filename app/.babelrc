{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "helpers": false,
      "regenerator": true,
      "absoluteRuntime": "@babel/runtime"
    }],
    "@babel/plugin-proposal-class-properties",
    "transform-flow-strip-types"
  ]
}